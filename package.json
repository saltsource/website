{
  "name": "saltsource-website",
  "version": "1.0.0",
  "description": "Isomorphic React/Redux App for the saltsource website",
  "main": "server.bundle.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --colors",
    "prestart": "npm run build",
    "start": "node server/server.bundle.js",
    "build:client": "webpack --quiet",
    "build:server": "webpack --quiet --config webpack.server.config.babel.js",
    "build": "npm run build:client && npm run build:server"
