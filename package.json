    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-require": "^2.1.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "classnames": "^2.2.1",
    "compression": "^1.6.1",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "express": "^4.13.4",
